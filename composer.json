{
    "name": "fivetwo/dependency-injection",
    "type": "library",
    "description": "The Five Two Dependency Injection Library",
    "keywords": ["dependency injection","dependency","injection","di","inversion of control","ioc"],
    "homepage": "https://www.fivetwo.org",
    "license": "proprietary",
    "authors": [
        {
            "name": "Matthew Suhocki",
            "email": "matt@fivetwo.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "FiveTwo\\DependencyInjection\\": "src/FiveTwo/DependencyInjection/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FiveTwo\\DependencyInjection\\": "tests/FiveTwo/DependencyInjection/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "ext-mbstring": "*",
        "ext-ctype":  "*",
        "ext-intl": "*",
        "fivetwo/remove-symfony-polyfill": "^8.1",
        "friendsofphp/php-cs-fixer": "^3.8.0",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": ">=0.16",
        "vimeo/psalm": "^4.23"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/fivetwo/remove-symfony-polyfill"
        }
    ],
    "scripts": {
        "test": "phpunit -c ."
    }
}
